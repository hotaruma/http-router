{
    "name": "hotaruma/http-router",
    "description": "Simple http router",
    "keywords": [
        "router",
        "php"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hotaruma\\HttpRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Max Denusenko",
            "email": "maxdenusenko@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "scripts": {
        "test": [
            "phpunit tests"
        ]
    }
}

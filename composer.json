{
    "name": "hotaruma/http-router",
    "description": "Simple http router",
    "keywords": [
        "router",
        "php"
    ],
    "homepage": "https://github.com/hotaruma/http-router",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Denusenko",
            "email": "maxdenusenko@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Hotaruma\\HttpRouter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.5",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "scripts": {
        "test": [
            "phpunit tests"
        ],
        "code-style": [
            "phpcs"
        ],
        "code-style-fix": [
            "phpcbf"
        ],
        "analyse": [
            "phpstan analyse"
        ],
        "php-coveralls": [
            "php-coveralls -v"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
